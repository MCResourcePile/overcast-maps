<?xml version="1.0"?>
<map proto="1.3.2">
<name>Easter Egg Hunt</name>
<version>1.0.0</version>
<objective>Capture all 10 eggs (wools) and return them to the victory monument!</objective>
<authors>
    <author>Reshif</author>
</authors>
<contributors>
    <contributor>Eclipsen</contributor>
    <contributor>Lyzak</contributor>
    <contributor>MonsieurApple</contributor>
    <contributor>Plastix</contributor>
    <contributor>IM_A_H0B0</contributor>
    <contributor>Anxuiz</contributor>
    <contributor>dewtroid</contributor>
    <contributor>MCSpider</contributor>
    <contributor>MCYukon</contributor>
    <contributor>pepsidawg00</contributor>
    <contributor>OurLoneHero</contributor>
    <contributor>Oversoul96</contributor>
    <contributor>SH4D0W_HAWK</contributor>
</contributors>
<rules>
    <rule>Don't be ridiculously cheap, skybridging 50 blocks away from the map is a bit silly</rule>
    <rule>I will punish people for the above rule using /slap or other methods ^</rule>
</rules>
<teams>
    <team color="gold" max="200">Players</team>
</teams>
<autorespawn/>
<spawns bedspawn="on">
    <spawns>
        <spawn team="Players" yaw="0"><cylinder base="1,22.2,0" radius="2" height="1"/></spawn>
    </spawns>
    <default yaw="0"><cylinder base="1,22.2,0" radius="2" height="1"/></default>
</spawns>
<filters>
    <filter name="only-torch">
        <block>torch</block>
    </filter>
</filters>
<regions>
    <apply block="only-torch">
        <rectangle name="spawn" min="-9,-10" max="11,10"/>
        <rectangle name="viridun" min="-oo,475" max="oo,oo"/>
    </apply>
</regions>
<mobs>
    <filter name="allow-all"/>
</mobs>
<wools team="players" craftable="false">
    <wool color="white"><block>-4,23,-10</block></wool>
    <wool color="magenta"><block>-3,23,-10</block></wool>
    <wool color="purple"><block>-2,23,-10</block></wool>
    <wool color="blue"><block>-1,23,-10</block></wool>
    <wool color="yellow"><block>0,23,-10</block></wool>
    <wool color="gray"><block>1,23,-10</block></wool>
    <wool color="red"><block>2,23,-10</block></wool>
    <wool color="light blue"><block>3,23,-10</block></wool>
    <wool color="cyan"><block>4,23,-10</block></wool>
    <wool color="green"><block>5,23,-10</block></wool>
</wools>
<portals>
    <portal x="@122" y="@32" z="@671" yaw="90">
        <cuboid min="-16,41,478" max="-10,47,479"/>
    </portal>
    <portal x="@-13" y="@42" z="@477" yaw="270">
        <cuboid min="123,31,668" max="124,37,674"/>
    </portal>
</portals>
<itemremove>
    <item>string</item>
    <item>red rose</item>
    <item>yellow flower</item>
</itemremove>
<friendlyfire>on</friendlyfire>
</map>